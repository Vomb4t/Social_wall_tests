<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="DEBUG" strict="true">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout
                    pattern="%highlight{[%-5level] %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] - %msg%n}{FATAL=red blink, ERROR=red, WARN=yellow bold, INFO=black, DEBUG=green bold, TRACE=blue}] [%t]: %logger{36}.%M@L%L - %msg%n" disableAnsi="false"/>/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="DEBUG" additivity="false">
            <AppenderRef ref="console"/>
        </Root>
    </Loggers>
</Configuration>